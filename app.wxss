.container {
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    padding: 200rpx 0;
    box-sizing: border-box;
}

.daohang {
    width: 100%;
    height: 150rpx;
    display: flex;
    align-items: center;
    padding: 50rpx 0 0 30rpx;
    position: relative;
    box-sizing: border-box;
}

.daohang image {
    width: 24px;
    height: 40rpx;
}

.daohang text {
    font-size: 35rpx;
    color: #fff;
    padding-left: 250rpx;
}