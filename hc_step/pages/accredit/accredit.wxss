.quah {
    width: 100%;
    height: 200rpx;
    padding: 15rpx;
    box-sizing: border-box;
    display: flex;
    justify-content: space-between;
    flex-direction: column;
}

.sdhaou {
    width: 100%;
    display: flex;
    justify-content: space-between;
    align-items: flex-end;
}

.shiay {
    width: 70%;
    color: #999;
    font-size: 25rpx;
    line-height: 53rpx;
}

.yia {
    color: #999;
    font-size: 27rpx;
}

.button-hover {
    background-color: none;
}

button {
    padding: 0;
    margin: 0;
    border-radius: 0;
    background: none;
    line-height: normal;
}

button::after {
    content: none;
}

.qiayu {
    width: 126rpx;
    height: 59rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    border: 2rpx solid #fa694c;
    color: #fa694c;
    font-size: 22rpx;
    border-radius: 20rpx;
}