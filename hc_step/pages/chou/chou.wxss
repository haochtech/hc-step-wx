#container {
    width: 571rpx;
    height: 556rpx;
    padding: 30rpx;
    box-sizing: border-box;
    position: relative;
    display: flex;
    flex-wrap: wrap;
    margin-top: 31rpx;
    padding-bottom: 49rpx;
}

.container_img {
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
    display: flex;
}

#li {
    width: 161rpx;
    height: 145rpx;
    display: flex;
    float: left;
    text-align: center;
    position: relative;
    background: #ffeeee;
    box-shadow: 1px 4px 0px #ffbfbf;
    border-radius: 25rpx;
    margin: 6rpx;
    margin-bottom: 0;
    flex-direction: column;
    justify-content: space-between;
    align-items: center;
    padding: 15rpx 0;
    padding-bottom: 20rpx;
    font-weight: 700;
    box-sizing: border-box;
}

#lil {
    width: 161rpx;
    height: 149rpx;
    margin: 6rpx;
    margin-bottom: 0;
    position: relative;
}

a {
    width: 161rpx;
    height: 145rpx;
    position: relative;
    margin: 6rpx;
    margin-top: 13rpx;
}

#li view {
    position: absolute;
}

#li image {
    width: 75rpx;
    height: 75rpx;
    display: block;
}

#li text {
    color: #6f2828;
    font-size: 21rpx;
    line-height: 0;
    height: 0;
}

a:hover {
    cursor: pointer;
    color: orange;
    font-size: 18px;
}

.active {
    background: red;
    color: #fff;
}

#pp {
    line-height: 32px;
    color: #9a9a9a;
    text-align: center;
}

page view .indexli {
    width: 160rpx;
    height: 150rpx;
