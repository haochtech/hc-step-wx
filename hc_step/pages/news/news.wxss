page {
    background: #f9f9f9;
    width: 100%;
    height: 100%;
}

.zanwu {
    color: #333333;
    font-size: 25rpx;
    width: 100%;
    height: 100%;
    padding: 30rpx;
    box-sizing: border-box;
    background: #fff;
}

.tongzxi {
    width: 100%;
    padding: 0 30rpx;
    box-sizing: border-box;
    height: 112rpx;
    display: flex;
    align-items: center;
    background: #fff;
    border-bottom: 2rpx solid #eaeaea;
    margin-top: 20rpx;
    justify-content: space-between;
}

.tongzxi image {
    width: 30rpx;
    height: 30rpx;
}

.tongzxi image:first-child {
    width: 78rpx;
    height: 78rpx;
}

.tongzxi text {
    color: #131313;
    font-size: 35rpx;
    margin-right: auto;
    margin-left: 28rpx;
}