@import "../../../wxParse/wxParse.wxss";

page {
}

view {
    word-break: normal!important;
    overflow: visible!important;
}

.wxParse {
    padding: 5px 0;
}

.more_gongis {
    width: 100%;
    height: 100rpx;
    box-sizing: border-box;
    padding: 25rpx;
    display: flex;
    background: #fff1da;
    position: fixed;
    bottom: 0;
    overflow: visible!important;
    align-items: center;
    justify-content: space-between;
}

.more_gongis text {
    color: #333333;
    font-size: 26rpx;
}

.more_gongis .more_yuas {
    padding: 0 25rpx;
    line-height: 60rpx;
    border-radius: 50rpx;
    background: linear-gradient(to right,#ff9024,#ff6209);
    color: #fff;
    font-size: 26rpx;
}