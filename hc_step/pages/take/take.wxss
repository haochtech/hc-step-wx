page {
    width: 100%;
    height: 100%;
    background: #f6f6f6;
}

.tianja {
    width: 100%;
    padding: 0 45rpx;
    box-sizing: border-box;
    position: fixed;
    bottom: 0;
    margin-bottom: 30rpx;
    display: flex;
    justify-content: space-between;
}

.tianja_ca {
    width: 320rpx;
    height: 92rpx;
    border-radius: 10rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}

.tianja_ca image {
    width: 35rpx;
    height: 35rpx;
}

.tianja_ca text {
    color: #ffffff;
    font-size: 30rpx;
    margin-left: 22rpx;
}

.notake {
    width: 100%;
    height: 385rpx;
    display: flex;
    align-items: center;
    flex-direction: column;
    justify-content: space-between;
    margin-top: 125rpx;
}

.notake image {
    width: 276rpx;
    height: 290rpx;
}

.notake text {
    color: #666666;
    font-size: 28rpx;
}

.dizhi {
    width: 100%;
    display: flex;
    flex-direction: column;
    background: #fff;
    margin-bottom: 20rpx;
}

.dizhi_yua {
    width: 100%;
    padding: 30rpx;
    box-sizing: border-box;
    display: flex;
    flex-direction: column;
}

.dizhi_yua text {
    color: #1e1e1f;
    font-size: 28rpx;
}

.dizhi_yua text:first-child {
    margin-top: 22rpx;
}

.xaimai {
    width: 100%;
    height: 85rpx;
    padding: 0 30rpx;
    box-sizing: border-box;
    display: flex;
    justify-content: space-between;
    border-top: 2rpx solid #f0f0f0;
}

.xaimai_ona {
    width: 178rpx;
    height: 100%;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.xaimai_ona image {
    width: 30rpx;
    height: 30rpx;
}

.xaimai_ona text {
    color: #9f04ff;
    font-size: 25rpx;
}

.huna {
    width: 202rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.buan {
    width: auto;
    height: 100%;
    display: flex;
    align-items: center;
}

.buan image {
    width: 23rpx;
    height: 30rpx;
}

.buan text {
    color: #9d9d9d;
    font-size: 25rpx;
    margin-left: 10rpx;
}

.puss {
    width: 90%;
    text-align: center;
    line-height: 80rpx;
    border-radius: 50rpx;
    border: 2rpx solid #7d6ae7;
    color: #7d6ae7;
    font-size: 28rpx;
    margin: 0 auto;
    margin-top: 500rpx;
}

.xaimai_ona .kan {
    color: #999;
    font-size: 25rpx;
}