page {
    position: relative;
    height: auto;
    height: 100%;
}

.release {
    padding-top: 26rpx;
    background: #fff;
    color: #fff;
    position: relative;
}

.RefundsBox {
    height: auto;
}

.textarea {
    padding: 0;
    min-height: 148rpx;
    height: auto;
    background: #fff;
    overflow-y: visible;
    margin-bottom: 57rpx;
    margin-left: 30rpx;
    color: #666;
}

.UploadVoucher {
    height: auto;
    border-bottom: none;
    padding-bottom: 30rpx;
}

.UploadImg {
    width: 100%;
    height: auto;
    padding: 0 30rpx;
    padding-right: 0;
    box-sizing: border-box;
    font-size: 0;
}

.UploadImg .Img {
    display: inline-block;
    width: 210rpx;
    height: 210rpx;
    margin-right: 30rpx;
    margin-bottom: 30rpx;
    border: none;
}

.AUImg {
    position: relative;
    border-radius: 10rpx;
    margin-right: 0;
    background: rgba(129,111,249,0.1);
}

.AUImg image {
    width: 100rpx;
    height: 90rpx;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
}

.AUImg view {
    height: 26rpx;
    line-height: 26rpx;
    top: 90rpx;
    font-size: 26rpx;
    width: 100%;
    text-align: center;
    position: absolute;
    color: rgb(134,134,134);
}

.AUImg .small {
    position: absolute;
    height: 20rpx;
    line-height: 20rpx;
    top: 126rpx;
    font-size: 20rpx;
}

.FUImg {
    position: relative;
    margin-right: 30rpx;
}

.voucher {
    width: 100%;
    height: 100%;
    border-radius: 10rpx;
    position: absolute;
    left: 0;
    top: 0;
}

.DELETE {
    position: absolute;
    width: 40rpx;
    height: 40rpx;
    right: -20rpx;
    top: -20rpx;
}

.addressBox {
    padding: 10rpx 30rpx;
    font-size: 0;
}

.addressInfo {
    display: inline-block;
    height: 50rpx;
    background: #f8f8f8;
    border-radius: 25rpx;
    padding: 0 30rpx 0 20rpx;
    font-size: 0;
    line-height: 1;
}

.addressInfo image {
    width: 30rpx;
    height: 30rpx;
    vertical-align: middle;
}

.addressInfo text {
    display: inline-block;
    padding-left: 10rpx;
    font-size: 26rpx;
    line-height: 50rpx;
    color: #999;
    vertical-align: middle;
}

.addressName text {
    display: inline-block;
    line-height: 50rpx;
    font-size: 26rpx;
    color: #7861ff;
    vertical-align: middle;
}

.addressName .delAddress {
    font-size: 22rpx;
    padding-left: 30rpx;
    display: inline-block;
}

.submit {
    position: absolute;
    bottom: 0;
    height: 90rpx;
}

.submit button {
    line-height: 90rpx;
    border-radius: 45rpx;
    font-size: 28rpx;
    color: #fff;
    background: linear-gradient(94deg,rgba(120,97,255,1),rgba(182,108,224,1));
}

.submit {
    width: 90%;
    height: 80rpx;
    position: fixed;
    bottom: 30rpx;
    color: #fff;
    left: 5%;
    border-radius: 40rpx;
}

.img-list {
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
    align-items: center;
    flex-wrap: wrap;
}

.img-item {
    width: 30%;
    text-align: left;
    position: relative;
    margin-right: 20rpx;
    margin-bottom: 10rpx;
}

.img-item image {
    width: 180rpx;
    height: 180rpx;
}

.img-item .delat {
    width: 40rpx;
    height: 40rpx;
    position: absolute;
    top: -15rpx;
    right: 15rpx;
}

.huatira {
    padding: 10rpx 30rpx;
    display: flex;
    align-items: center;
    margin-top: 25rpx;
}

.huatira image {
    width: 45rpx;
    height: 45rpx;
}

.huatira text {
    font-weight: 700;
    color: #666;
    font-size: 28rpx;
    margin-left: 30rpx;
}

.huati {
    padding: 10rpx 30rpx;
    padding-bottom: 120rpx;
}

.huati text {
    padding: 4rpx 8rpx;
    border: 1rpx solid #999;
    border-radius: 8rpx;
    color: #666;
    margin-right: 20rpx;
    font-size: 25rpx;
    display: inline-block;
    margin-bottom: 10rpx;
}

.topic {
    padding: 10rpx 30rpx;
}

.topic_one {
    display: flex;
    align-items: center;
}

.topic_t {
    color: rgba(120,97,255,1);
    border: 1px solid rgba(120,97,255,1);
    width: 140rpx;
    height: 40rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    border-radius: 50rpx;
    font-size: 20rpx;
}

.topic_uis {
    color: rgba(120,97,255,1);
    height: 40rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    border-radius: 50rpx;
    font-size: 25rpx;
    margin-right: 10rpx;
}

.topic_one image {
    width: 50rpx;
    height: 50rpx;
    box-sizing: border-box;
    padding: 10rpx;
}

.topic_can {
    color: #fb6e20;
    font-size: 18rpx;
    padding-left: 18rpx;
}

.huati .huati_zi {
    background: rgba(120,97,255,1);
    color: #fff;
    border: 1rpx solid rgba(120,97,255,1);
}