.time {
    width: 158rpx;
    height: 60rpx;
    background: #e9e9e9;
    display: flex;
    align-items: center;
    justify-content: center;
    color: #333333;
    font-size: 22rpx;
    border-top-right-radius: 50rpx;
    border-bottom-right-radius: 50rpx;
    margin-top: 63rpx;
}

.luhfdu {
    width: 100%;
    padding: 0 44rpx;
    box-sizing: border-box;
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-top: 50rpx;
}

.luhfdu .ains {
    color: #eb1111;
    font-size: 30rpx;
    margin-right: auto;
    margin-left: 30rpx;
}

.luhfdu text {
    color: #666666;
    font-size: 26rpx;
}

.xsavcad {
    color: #999999;
    font-size: 25rpx;
    margin: 0 auto;
    width: 180rpx;
    position: relative;
    margin-top: 79rpx;
}

.xsavcad::before {
    content: '';
    width: 27rpx;
    height: 0;
    border: #c4c4c4 2rpx solid;
    position: absolute;
    left: -50rpx;
    top: 15rpx;
}

.xsavcad::after {
    content: '';
    width: 27rpx;
    height: 0;
    border: #c4c4c4 2rpx solid;
    position: absolute;
    right: -50rpx;
    top: 15rpx;
}

.main {
    width: 100%;
}